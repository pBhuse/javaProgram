package NonPrimitiveArrayImplementation;

public class StudentArray
{
public static void main(String[] args) 
{
	//Create The Object Of Student Class
	Student obj1=new Student();
	Student obj2=new Student();
	Student obj3=new Student();
	Student obj4=new Student();
    obj1.rollNo=101;
    obj1.name="Pratiksha";
    //obj1.rollNo=102;
    //obj1.name="Reshma";
    obj2.rollNo=102;
    obj2.name="Reshma";
    obj3.rollNo=103;
    obj3.name="Smita";
    obj4.rollNo=104;
    obj4.name="Sanmesh";
    AnotherStudent obj=new AnotherStudent();
	obj.PrintArray();

	Student[]StudArray=new Student[5];// We create Student Type Of Array.
	StudArray[0]=obj1;
	StudArray[1]=obj2;
	StudArray[2]=obj3;
	StudArray[3]=obj4;
}

void getStudent(Student []arr )
{
	for(int i=0;i<arr.length;i++)
	{
		Student sobj=arr[i];
	
	System.out.println("RollNo:-"+sobj.rollNo);
	System.out.println("Name:-"+sobj.name);
}
}

}